body{
background-color:#fff;
margin:0;
}

div,td {
	font-family: "ו";
	font-size: 12px;
	color: #333333;
}
clear{
clear:both;
}
a{
color:#000000;
text-decoration:none;
}
a:hover{
	text-decoration: underline;
}
.a_blue{
color:#006eb3;
text-decoration:none;
}
.a_blue:hover{
color:#e17009;
text-decoration:none;
}
.a_blue2{
color:#a0c9e4;
text-decoration:none;
}
.a_blue2:hover{
color:#006eb3;
text-decoration:none;
}
.a_white{
color:#fff;
text-decoration:none;
}
.a_white:hover{
color:#fff;
text-decoration: underline;
}


.fB {font-weight: bold;}
.fI {font-style: italic;}
.f12px{ font-size:12px;}
.f14px{ font-size:14px;}

.left-new{ float:left;}
.right-new{ float:right;}


.top-new{
clear:both;
height:60px;
}
.top {
background-color:#bfd2dd;
background-image:url(../images/logo.png);
height:73px;
width:1000px;
background-repeat:no-repeat;
}
.logo{ 
float:left;
height:60px;
width:380px;
background-image:url(../images/logo.png);
background-position:10px 5px;
background-repeat:no-repeat;
}
.logo_text{
float:right;
color:#0d3b63;
text-align:center;
margin-top:15px;
margin-right:10px;
width:240px;

font-size: 14px;

}
.menu{
background-image:url(../images/menubg.jpg);
background-repeat:repeat-x;
height:32px;
line-height:32px;
clear:both;
}
.menutext{
float:left;
margin-left:3px;
margin-right:3px;
color:#FFFFFF;
font-weight: bold;
text-align:center;
width:80px;
}
.menutextdown{
float:left;
margin-left:3px;
margin-right:3px;
background-image:url(../images/menubg-down.jpg);
background-repeat:no-repeat;
background-position:center center;
color:#0d3b63;
font-weight: bold;
text-align:center;
width:80px;
}
.menutexthover{
float:left;
margin-left:3px;
margin-right:3px;
background-image:url(../images/menubg-hover.jpg);
background-repeat:no-repeat;
background-position:center center;
color:#fff;
font-weight: bold;
text-align:center;
width:80px;
}
.menu2{
background-image:url(../images/menubg2.jpg);
background-repeat:repeat-x;
height:8px;
clear:both;
}

.main{
clear:both;
margin-top:10px;
}
.mainleft{
float:left;
width:195px;
}
.mainleft_tdtop{
color:#fff;
line-height:26px;
