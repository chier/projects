}
.mailrightanswer table td {
padding:0 3px 0 3px;
	height: 26px;
	color:#515151;
	height:24px;
	line-height:24px;
	text-align:center;
}
.mainrightanswer_trgrey{
background-color:#f3f3f3;
}
.mainrightanswer_trdown{
background-color:#fbec88;
}

.mailrightanswer table th{
height:24px;
