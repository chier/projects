<!DOCTYPE struts PUBLIC
          "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
          "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
<package name="struts-model" namespace="/manage/common/model" extends="struts-interceptor">
		<!-- 模块的Action -->
		<action name="listModelTree" class="listModelTree">
			<result name="success" type="dispatcher">/html/webadmin/default.jsp</result>
			<result name="initRoot" type="dispatcher">/html/model/model.jsp</result>
			<result name="modelList" type="dispatcher">/html/adminSetting/model.jsp</result>
			<interceptor-ref name="common-params"/>
		</action>
		
	
	</package>
</struts>